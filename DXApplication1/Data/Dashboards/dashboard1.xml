<?xml version="1.0" encoding="utf-8"?>
<Dashboard>
  <Title Text="Sample Dashboard" />
  <DataSources>
    <SqlDataSource Name="SQL Data Source" ComponentName="sqlDataSource1" DataProcessingMode="Client">
      <Connection Name="NWindConnectionString" FromAppConfig="true" />
      <Query Type="SelectQuery" Name="Products_Categories">
        <Tables>
          <Table Name="Categories" />
          <Table Name="Products" />
          <Relation Type="Inner" Parent="Categories" Nested="Products">
            <KeyColumn Parent="CategoryID" Nested="CategoryID" />
          </Relation>
        </Tables>
        <Columns>
          <AllColumns Table="Categories" />
          <AllColumns Table="Products" />
        </Columns>
      </Query>
      <ConnectionOptions CloseConnection="true" />
    </SqlDataSource>
  </DataSources>
  <Items>
    <Chart ComponentName="chartDashboardItem1" Name="Chart 1">
      <Panes>
        <Pane Name="Pane 1" />
      </Panes>
    </Chart>
    <Chart ComponentName="chartDashboardItem2" Name="Chart 2">
      <Panes>
        <Pane Name="Pane 1" />
      </Panes>
    </Chart>
  </Items>
  <LayoutTree>
    <LayoutGroup>
      <LayoutItem DashboardItem="chartDashboardItem1" />
      <LayoutItem DashboardItem="chartDashboardItem2" />
    </LayoutGroup>
  </LayoutTree>
</Dashboard>